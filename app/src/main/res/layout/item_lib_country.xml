<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    android:orientation="horizontal"
    android:weightSum="41"
>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="4"
    >
        <TextView
            android:id="@+id/tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="序号"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv2"
        android:layout_toRightOf="@+id/tv1"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="7"
    >
        <TextView
            android:id="@+id/tv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="行政区划"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv3"
        android:layout_toRightOf="@+id/tv2"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="6"
    >
        <TextView
            android:id="@+id/tv3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="已治理数量"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv4"
        android:layout_toRightOf="@+id/tv3"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="6"
    >
        <TextView
            android:id="@+id/tv4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="未治理数量"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv5"
        android:layout_toRightOf="@+id/tv4"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="6"
    >
        <TextView
            android:id="@+id/tv5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="小计"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv6"
        android:layout_toRightOf="@+id/tv5"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="6"
    >
        <TextView
            android:id="@+id/tv6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="小计"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

    <View
        style="@style/divider_vertical3"
        android:layout_centerVertical="true"
        android:layout_toLeftOf="@+id/tv7"
        android:layout_toRightOf="@+id/tv6"/>

    <RelativeLayout
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_weight="6"
    >
        <TextView
            android:id="@+id/tv7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:text="小计"
            android:textColor="#444444"
            android:textSize="11sp"
        />

    </RelativeLayout>

</LinearLayout>